{
	"name": "svelte-next",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"build:node": "cp ./svelte.node.config.js  svelte.config.js && svelte-kit build",
		"build:static": "cp ./svelte.static.config.js  svelte.config.js && svelte-kit build",
		"static": "sirv build",
		"db": "json-server db.json --port 4000"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "0.0.16",
		"@sveltejs/adapter-static": "0.0.16",
		"@sveltejs/kit": "0.0.27",
		"@sveltejs/snowpack-config": "0.0.7",
		"svelte": "^3.30.0",
		"typescript": "^4.1.2",
		"tslib": "^2.0.3",
		"svelte-preprocess": "^4.6.1",
		"@snowpack/plugin-typescript": "^1.1.0",
		"sass": "^1.29.0",
		"json-server": "^0.16.3",
		"sirv-cli": "^1.0.8"
	}
}
