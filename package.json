{
	"name": "svelte-next",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"build:node": "cp ./svelte.node.config.js svelte.config.js && svelte-kit build && svelte-kit adapt",
		"build:static": "cp ./svelte.static.config.js svelte.config.js && svelte-kit build && svelte-kit adapt",
		"static": "sirv build",
		"db": "json-server db.json --port 4000",
		"adapt": "svelte-kit adapt"
	},
	"devDependencies": {
		"@snowpack/plugin-typescript": "^1.1.1",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/snowpack-config": "next",
		"@sveltejs/app-utils": "next",
		"json-server": "^0.16.3",
		"sass": "^1.30.0",
		"sirv-cli": "^1.0.10",
		"svelte": "^3.31.0",
		"svelte-preprocess": "^4.6.1",
		"tslib": "^2.0.3",
		"typescript": "^4.1.2"
	}
}
